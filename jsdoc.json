{
    "source": {
      "include": ["src", "server.js"], 
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
      "destination": "./docs",
      "recurse": true,
      "readme": "README.md"
    },
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    }
  }